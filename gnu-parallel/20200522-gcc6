module-whatis "GNU parallel is a shell tool for executing jobs in parallel using one or more computers."

proc ModulesHelp { } {
puts stderr "GNU parallel is a shell tool for executing jobs in parallel using one or more computers."
puts stderr "See https://www.gnu.org/software/parallel/ ."
}

prepend-path PATH "/mnt/lustre/indy2lfs/sw/gnu-parallel/20200522/bin"
prepend-path MANPATH "/mnt/lustre/indy2lfs/sw/gnu-parallel/20200522/share/man"
