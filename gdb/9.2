#%Module######################################################################
##
##      gdb 9.2
##
module-whatis "GDB@9.2"
proc ModulesHelp { } {
  puts stderr "GDB 9.2"
  puts stderr "=======\n"

  puts stderr "This module adds GDB to your environment."

  puts stderr "Installation instructions can be found at:\n"

  puts stderr "\t* http://sourceware.org/gdb/current/onlinedocs/gdb/Installing-GDB.html\n"

  puts stderr "\tInstalled: 21 August 2020"
  puts stderr "\tInstalled by: William Lucas, EPCC"
}

set base        /mnt/lustre/indy2lfs/sw/gdb/9.2
set ver         9.2
set path        $base

prepend-path PATH	      $path/bin
prepend-path LD_LIBRARY_PATH  $path/lib
prepend-path CPATH            $path/include
prepend-path MANPAGE          $path/share/man
