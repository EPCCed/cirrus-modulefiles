#%Module
# 

# Clashes
conflict autoconf

# The text to print if someone types "module help autoconf/2.71"
proc ModulesHelp { } {
puts stderr "\t autoconf 2.71"
puts stderr "\t=============\n"
}

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys        [uname sysname]

set ROOT /work/y07/shared/cirrus-software/autoconf/2.71

prepend-path PATH $ROOT/bin
prepend-path MANPATH $ROOT/share/man
prepend-path ACLOCAL_PATH $ROOT/share/autoconf
