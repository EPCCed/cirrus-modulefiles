#%Module
# 
# Module definition for libtool 2.4.6
#
# M. Bareford, EPCC, 4 August 2020

# Clashes
conflict libtool

# The text to print if someone types "module help libtool/2.4.6"
proc ModulesHelp { } {
puts stderr "\tlibtool 2.4.6"
puts stderr "\t=============\n"
puts stderr "\tInstalled by M. Bareford, EPCC"
puts stderr "\tCompiled with gcc 8.2.0"
puts stderr "\tDate: 4 August 2020\n"
}

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys        [uname sysname]

set ROOT /mnt/lustre/indy2lfs/sw/libtool/2.4.6

prepend-path PATH $ROOT/bin
prepend-path CPATH $ROOT/include
prepend-path MANPATH $ROOT/share/man
prepend-path ACLOCAL_PATH $ROOT/share/aclocal

prepend-path LIBRARY_PATH $ROOT/lib
prepend-path LD_LIBRARY_PATH $ROOT/lib
prepend-path LD_RUN_PATH $ROOT/lib

